#!/usr/bin/env nu

# ./cli ..

def help [] {
  $'
test manual
  '
  | str trim
}

def main [
  -h
  ...args: string
] {
  if $h {
    help | less --RAW-CONTROL-CHARS
    return
  }

  echo ...$args
}

def 'main test manual' [] {
  ./cli convert bitwarden-to-apple ./src/__tests__/fixtures/bitwarden.json data/output.csv
}